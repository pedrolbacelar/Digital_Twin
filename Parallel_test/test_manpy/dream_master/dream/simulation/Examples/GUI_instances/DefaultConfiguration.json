          "properties": {
            "left": "number",
            "top": "number"
          },
          "type": "object"
        },
        "id": {
          "description": "The unique identifier of this node",
          "name": "ID",
          "priority": 10000,
          "type": "string"
        },
        "name": {
          "description": "The name of this node",
